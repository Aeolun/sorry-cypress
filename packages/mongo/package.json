{
  "name": "@sorry-cypress/mongo",
  "version": "1.0.0",
  "author": {
    "name": "Andrew Goldis",
    "email": "agoldis@gmail.com"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/",
  "scripts": {
    "dev": "run-p js:watch types:watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly --declarationMap",
    "types:watch": "yarn run build:types --watch --preserveWatchOutput",
    "js:watch": "yarn build:js --watch",
    "build:js": "esbuild ./src/index.ts --bundle  --platform=node --sourcemap --outdir=dist",
    "lint": "eslint ./ --ext ts,tsx",
    "release": "echo \"skipping release\"",
    "test": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "esbuild": "^0.11.14",
    "jest": "^26.6.3"
  },
  "dependencies": {
    "@sorry-cypress/common": "1.0.0",
    "date-fns": "^2.21.1",
    "mongodb": "^3.6.5",
    "mongodb-client-encryption": "^1.2.3"
  }
}
